{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "sourceType": "script"
    },
    "rules": {
        "strict": "warn",
        "semi": ["warn", "always"],
        "quotes": ["error", "double"],
        "no-alert": "off",
        "no-console": "off",
        "no-unused-vars": "warn"
    }
}